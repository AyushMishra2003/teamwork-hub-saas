generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Plasma {
  id          Int      @id @default(autoincrement())
  name        String
  email       String
  phoneNumber String
}


model User {
  id          Int      @id @default(autoincrement())
  name        String
  email       String @unique
  phoneNumber String
  age          Int
  bussinessMail String?@default("ayushm185@gmail.com")
}






